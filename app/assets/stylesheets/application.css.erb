 * or vendor/assets/stylesheets of plugins, if any, can be referenced here using a relative path.
 *
 * You're free to add application-wide styles to this file and they'll appear at the top of the
 * compiled file, but it's generally better to create a new file per style scope.
 *
 *= require_self
 *= require_tree .
 */

@font-face {
    font-family: 'prettyCursive';
    src: url('<%= asset_path 'janeaust.eot' %>');
    src: url('<%= asset_path 'janeaust.eot?#iefix' %>') format('embedded-opentype'),
    url('<%= asset_path 'janeaust.woff' %>') format('woff'),
    url('<%= asset_path 'janeaust.ttf' %>') format('truetype'),
    url('<%= asset_path 'janeaust.svg' %>') format('svg');
}

@font-face {
    font-family: 'usDeclaration';
    src: url('<%= asset_path 'usdeclaration-webfont.eot' %>');
    src: url('<%= asset_path 'usdeclaration-webfont.eot?#iefix' %>') format('embedded-opentype'),
    url('<%= asset_path 'usdeclaration-webfont.woff' %>') format('woff'),
    url('<%= asset_path 'usdeclaration-webfont.ttf' %>') format('truetype'),
    url('<%= asset_path 'usdeclaration-webfont.svg' %>') format('svg');
}