@font-face {
    font-family: 'prettyCursive';
    src: url('<%= asset_path 'janeaust.eot' %>');
    src: url('<%= asset_path 'janeaust.eot?#iefix' %>') format('embedded-opentype'),
    url('<%= asset_path 'janeaust.woff' %>') format('woff'),
    url('<%= asset_path 'janeaust.ttf' %>') format('truetype'),
    url('<%= asset_path 'janeaust.svg' %>') format('svg');
}


